{
  "version": 2,
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/prescription/:id",
      "dest": "src/prescription.controller.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/api/medicine/:id",
      "dest": "src/medicine.controller.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
